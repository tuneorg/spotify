{
  "name": "@tuneorg/spotify",
  "version": "2.0.1",
  "private": false,
  "description": "A spotify resolver",
  "keywords": [
    "spotify",
    "discord",
    "bot",
    "music",
    "spotify-api"
  ],
  "homepage": "https://github.com/tuneorg/spotify#readme",
  "bugs": {
    "url": "https://github.com/tuneorg/spotify/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuneorg/spotify.git"
  },
  "license": "MIT",
  "author": "Tune <contact@tunebot.org>",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run lint && npm run compile",
    "compile": "tsc --build tsconfig.json",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "start": "ts-node src/index.ts"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": "@hazmi35/eslint-config/typescript/node",
    "rules": {
      "@typescript-eslint/no-unsafe-member-access": "off",
      "@typescript-eslint/no-unsafe-call": "off",
      "multiline-comment-style": "off"
    }
  },
  "dependencies": {
    "undici": "^5.21.0"
  },
  "devDependencies": {
    "@hazmi35/eslint-config": "^9.0.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16.6.0"
  }
}
